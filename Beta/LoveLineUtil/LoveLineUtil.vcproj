<?xml version="1.0"?>
<VisualStudioProject
	ProjectType="Visual C++"
	Version="7.00"
	Name="LoveLineUtil">
	<Build>
		<Settings>
			<Platform
				Name="Win32"/>
			<Configuration
				Name="Debug|Win32"
				IntermediateDirectory="obj"
				OutputDirectory="..\bin"
				ConfigurationType="2"
				CharacterSet="1">
				<Tool
					Name="VCBscMakeTool"/>
				<Tool
					Name="VCCLCompilerTool"
					Optimization="0"
					Defines="_WIN32_WINNT=0x0500;WINVER=0x0500;WIN32;UNICODE;_UNICODE;DEBUG"
					MinimalRebuild="TRUE"
					FullRuntimeChecks="TRUE"
					RuntimeLibrary="1"
					UsePrecompiledHeader="0"
					WarnLevel="3"
					SuppressStartupMessage="TRUE"
					DebugInformation="4"/>
				<Tool
					Name="VCCustomBuildTool"/>
				<Tool
					Name="VCLinkerTool"
					AdditionalInputs="wmstub.lib vfw32.lib wmvcore.lib odbc32.lib odbccp32.lib shlwapi.lib rpcrt4.lib dsound.lib quartz.lib strmbase.lib comctl32.lib"
					OutputFile="$(OutDir)/LoveLineUtil.dll"
					LinkIncremental="2"
					SuppressStartupMessage="TRUE"
					IgnoreDefaultLibraries="FALSE"
					GenerateDebug="TRUE"
					ProgramDatabaseFile="$(OutDir)/LoveLineUtil.pdb"
					SubSystem="2"
					ImportLibrary="$(OutDir)/LoveLineUtil.lib"
					TargetMachine="1"/>
				<Tool
					Name="VCMIDLTool"
					TargetEnvironment="1"/>
				<Tool
					Name="VCPostBuildEventTool"/>
				<Tool
					Name="VCPreBuildEventTool"/>
				<Tool
					Name="VCPreLinkEventTool"/>
				<Tool
					Name="VCResourceCompilerTool"/>
				<Tool
					Name="VCSDLTool"/>
			</Configuration>
			<Configuration
				Name="Release|Win32"
				IntermediateDirectory="obj"
				OutputDirectory="..\bin"
				ConfigurationType="2"
				CharacterSet="1">
				<Tool
					Name="VCBscMakeTool"/>
				<Tool
					Name="VCCLCompilerTool"
					Optimization="2"
					InlineFunctionExpansion="1"
					FramePointerOmission="TRUE"
					Defines="_WIN32_WINNT=0x0500;WINVER=0x0500;WIN32;UNICODE;_UNICODE;NDEBUG"
					StringPooling="TRUE"
					RuntimeLibrary="0"
					PackageFunctions="TRUE"
					UsePrecompiledHeader="0"
					WarnLevel="3"
					SuppressStartupMessage="TRUE"
					DebugInformation="3"/>
				<Tool
					Name="VCCustomBuildTool"/>
				<Tool
					Name="VCLinkerTool"
					AdditionalInputs="wmstub.lib vfw32.lib wmvcore.lib odbc32.lib odbccp32.lib shlwapi.lib rpcrt4.lib dsound.lib quartz.lib strmbase.lib comctl32.lib"
					OutputFile="$(OutDir)/LoveLineUtil.dll"
					LinkIncremental="1"
					SuppressStartupMessage="TRUE"
					IgnoreDefaultLibraries="FALSE"
					GenerateDebug="FALSE"
					SubSystem="2"
					ImportLibrary="$(OutDir)/LoveLineUtil.lib"
					TargetMachine="1"/>
				<Tool
					Name="VCMIDLTool"
					TargetEnvironment="1"/>
				<Tool
					Name="VCPostBuildEventTool"/>
				<Tool
					Name="VCPreBuildEventTool"/>
				<Tool
					Name="VCPreLinkEventTool"/>
				<Tool
					Name="VCResourceCompilerTool"/>
				<Tool
					Name="VCSDLTool"/>
			</Configuration>
		</Settings>
	</Build>
