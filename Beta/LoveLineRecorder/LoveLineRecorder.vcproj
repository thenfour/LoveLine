<?xml version="1.0"?>
<VisualStudioProject
	ProjectType="Visual C++"
	Version="7.00"
	Name="LoveLineRecorder">
	<Build>
		<Settings>
			<Platform
				Name="Win32"/>
			<Configuration
				Name="Debug|Win32"
				IntermediateDirectory="obj"
				OutputDirectory="..\bin"
				ConfigurationType="1"
				CharacterSet="1">
				<Tool
					Name="VCBscMakeTool"/>
				<Tool
					Name="VCCLCompilerTool"
					Optimization="0"
					Defines="WIN32;_DEBUG;_WINDOWS,_UNICODE,UNICODE"
					MinimalRebuild="TRUE"
					FullRuntimeChecks="TRUE"
					RuntimeLibrary="3"
					UsePrecompiledHeader="0"
					WarnLevel="3"
					SuppressStartupMessage="TRUE"
					DebugInformation="4"/>
				<Tool
					Name="VCCustomBuildTool"/>
				<Tool
					Name="VCLinkerTool"
					AdditionalInputs="comctl32.lib shlwapi.lib"
					OutputFile="$(OutDir)/LoveLineRecorder.exe"
					LinkIncremental="2"
					SuppressStartupMessage="TRUE"
					IgnoreDefaultLibraries="FALSE"
					GenerateDebug="TRUE"
					ProgramDatabaseFile="$(OutDir)/LoveLineRecorder.pdb"
					SubSystem="2"
					OptimizeForWindows98="2"
					TargetMachine="1"/>
				<Tool
					Name="VCMIDLTool"
					TargetEnvironment="1"/>
				<Tool
					Name="VCPostBuildEventTool"/>
				<Tool
					Name="VCPreBuildEventTool"/>
				<Tool
					Name="VCPreLinkEventTool"/>
				<Tool
					Name="VCResourceCompilerTool"/>
				<Tool
					Name="VCSDLTool"/>
			</Configuration>
			<Configuration
				Name="Release|Win32"
				IntermediateDirectory="obj"
				OutputDirectory="..\bin"
				ConfigurationType="1"
				CharacterSet="2">
				<Tool
					Name="VCBscMakeTool"/>
				<Tool
					Name="VCCLCompilerTool"
					Optimization="2"
					InlineFunctionExpansion="1"
					FramePointerOmission="TRUE"
					Defines="WIN32;NDEBUG;_WINDOWS,_UNICODE,UNICODE"
					StringPooling="TRUE"
					RuntimeLibrary="2"
					PackageFunctions="TRUE"
					UsePrecompiledHeader="0"
					WarnLevel="3"
					SuppressStartupMessage="TRUE"
					DebugInformation="3"/>
				<Tool
					Name="VCCustomBuildTool"/>
				<Tool
					Name="VCLinkerTool"
					AdditionalInputs="comctl32.lib shlwapi.lib"
					OutputFile="$(OutDir)/LoveLineRecorder.exe"
					LinkIncremental="1"
					SuppressStartupMessage="TRUE"
					GenerateDebug="FALSE"
					SubSystem="2"
					OptimizeForWindows98="1"
					TargetMachine="1"/>
				<Tool
					Name="VCMIDLTool"
					TargetEnvironment="1"/>
				<Tool
					Name="VCPostBuildEventTool"/>
				<Tool
					Name="VCPreBuildEventTool"/>
				<Tool
					Name="VCPreLinkEventTool"/>
				<Tool
					Name="VCResourceCompilerTool"/>
				<Tool
					Name="VCSDLTool"/>
			</Configuration>
		</Settings>
	</Build>
	<Files>
		<Filter
			Name="Source Files"
			Filter="cpp;c;cxx;rc;def;odl;idl;hpj;bat;asm">
			<File
				RelativePath="LLApp.cpp">
			</File>
			<File
				RelativePath="Recorder.rc">
			</File>
			<File
				RelativePath="RecorderMain.cpp">
			</File>
		</Filter>
		<Filter
			Name="Header Files"
			Filter="h;hpp;hxx;hm;inl;inc">
			<File
				RelativePath="LLApp.h">
			</File>
			<File
				RelativePath="resource.h">
			</File>
		</Filter>
		<Filter
			Name="Resource Files"
			Filter="ico;cur;bmp;dlg;rc2;rct;bin;rgs;gif;jpg;jpeg;jpe">
			<File
				RelativePath="Header.bmp">
			</File>
			<File
				RelativePath="Icon.ico">
			</File>
			<File
				RelativePath="Watermark.bmp">
			</File>
		</Filter>
		<File
			RelativePath="readme.txt">
		</File>
	</Files>
	<Globals>
	</Globals>
</VisualStudioProject>
