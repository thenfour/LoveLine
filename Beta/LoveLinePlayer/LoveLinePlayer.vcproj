<?xml version="1.0"?>
<VisualStudioProject
	ProjectType="Visual C++"
	Version="7.00"
	Name="LoveLinePlayer">
	<Build>
		<Settings>
			<Platform
				Name="Win32"/>
			<Configuration
				Name="Debug|Win32"
				IntermediateDirectory="obj"
				OutputDirectory="..\bin"
				ConfigurationType="1"
				CharacterSet="1">
				<Tool
					Name="VCBscMakeTool"/>
				<Tool
					Name="VCCLCompilerTool"
					Optimization="0"
					Defines="UNICODE,_UNICODE,_WIN32_WINNT=0x0500,WINVER=0x0500"
					MinimalRebuild="TRUE"
					FullRuntimeChecks="TRUE"
					RuntimeLibrary="5"
					UsePrecompiledHeader="0"
					WarnLevel="3"
					SuppressStartupMessage="TRUE"
					DebugInformation="4"/>
				<Tool
					Name="VCCustomBuildTool"/>
				<Tool
					Name="VCLinkerTool"
					AdditionalInputs="kernel32.lib user32.lib gdi32.lib winspool.lib comdlg32.lib advapi32.lib shell32.lib ole32.lib oleaut32.lib uuid.lib odbc32.lib odbccp32.lib shlwapi.lib rpcrt4.lib"
					OutputFile="$(OutDir)/LoveLinePlayer.exe"
					LinkIncremental="2"
					SuppressStartupMessage="TRUE"
					GenerateDebug="TRUE"
					ProgramDatabaseFile="$(OutDir)/LoveLinePlayer.pdb"
					SubSystem="2"
					TargetMachine="1"/>
				<Tool
					Name="VCMIDLTool"
					TargetEnvironment="1"/>
				<Tool
					Name="VCPostBuildEventTool"/>
				<Tool
					Name="VCPreBuildEventTool"/>
				<Tool
					Name="VCPreLinkEventTool"/>
				<Tool
					Name="VCResourceCompilerTool"/>
				<Tool
					Name="VCSDLTool"/>
			</Configuration>
			<Configuration
				Name="Release|Win32"
				IntermediateDirectory="obj"
				OutputDirectory="..\bin"
				ConfigurationType="1"
				CharacterSet="1">
				<Tool
					Name="VCBscMakeTool"/>
				<Tool
					Name="VCCLCompilerTool"
					Optimization="2"
					InlineFunctionExpansion="1"
					FramePointerOmission="TRUE"
					Defines="UNICODE,_UNICODE,_WIN32_WINNT=0x0500,WINVER=0x0500"
					StringPooling="TRUE"
					RuntimeLibrary="4"
					PackageFunctions="TRUE"
					UsePrecompiledHeader="0"
					WarnLevel="3"
					SuppressStartupMessage="TRUE"
					DebugInformation="3"/>
				<Tool
					Name="VCCustomBuildTool"/>
				<Tool
					Name="VCLinkerTool"
					AdditionalInputs="kernel32.lib user32.lib gdi32.lib winspool.lib comdlg32.lib advapi32.lib shell32.lib ole32.lib oleaut32.lib uuid.lib odbc32.lib odbccp32.lib shlwapi.lib rpcrt4.lib"
					OutputFile="$(OutDir)/LoveLinePlayer.exe"
					LinkIncremental="1"
					SuppressStartupMessage="TRUE"
					GenerateDebug="FALSE"
					SubSystem="2"
					TargetMachine="1"/>
				<Tool
					Name="VCMIDLTool"
					TargetEnvironment="1"/>
				<Tool
					Name="VCPostBuildEventTool"/>
				<Tool
					Name="VCPreBuildEventTool"/>
				<Tool
					Name="VCPreLinkEventTool"/>
				<Tool
					Name="VCResourceCompilerTool"/>
				<Tool
					Name="VCSDLTool"/>
			</Configuration>
		</Settings>
	</Build>
	<Files>
		<Filter
			Name="Source Files"
			Filter="cpp;c;cxx;rc;def;odl;idl;hpj;bat;asm">
			<File
				RelativePath="LLNavigation.cpp">
			</File>
			<File
				RelativePath="LLPlayerApp.cpp">
			</File>
			<File
				RelativePath="LLPlayerCore.cpp">
			</File>
			<File
				RelativePath="P_Episode.cpp">
			</File>
			<File
				RelativePath="P_Features.cpp">
			</File>
			<File
				RelativePath="P_Library.cpp">
			</File>
			<File
				RelativePath="P_Links.cpp">
			</File>
			<File
				RelativePath="P_Playback.cpp">
			</File>
			<File
				RelativePath="Player.rc">
			</File>
			<File
				RelativePath="PlayerMain.cpp">
			</File>
			<File
				RelativePath="StandardPlugins.cpp">
			</File>
		</Filter>
		<Filter
			Name="Header Files"
			Filter="h;hpp;hxx;hm;inl;inc">
			<File
				RelativePath="LLNavigation.h">
			</File>
			<File
				RelativePath="LLPlayerApp.h">
			</File>
			<File
				RelativePath="LLPlayerCore.h">
			</File>
			<File
				RelativePath="LLPlugin.h">
			</File>
			<File
				RelativePath="LLPluginInformation.h">
			</File>
			<File
				RelativePath="P_Episode.h">
			</File>
			<File
				RelativePath="P_Features.h">
			</File>
			<File
				RelativePath="P_Library.h">
			</File>
			<File
				RelativePath="P_Links.h">
			</File>
			<File
				RelativePath="P_Playback.h">
			</File>
			<File
				RelativePath="resource.h">
			</File>
			<File
				RelativePath="StandardPlugins.h">
			</File>
		</Filter>
		<Filter
			Name="Resource Files"
			Filter="ico;cur;bmp;dlg;rc2;rct;bin;rgs;gif;jpg;jpeg;jpe">
			<File
				RelativePath="Back.bmp">
			</File>
			<File
				RelativePath="Commercial.bmp">
			</File>
			<File
				RelativePath="Forward.bmp">
			</File>
			<File
				RelativePath="Icon.ico">
			</File>
			<File
				RelativePath="Icon_Episode.bmp">
			</File>
			<File
				RelativePath="Icon_Feature.bmp">
			</File>
			<File
				RelativePath="Play.bmp">
			</File>
			<File
				RelativePath="Splash.bmp">
			</File>
			<File
				RelativePath="Stop.bmp">
			</File>
		</Filter>
		<File
			RelativePath="Player.Txt">
		</File>
	</Files>
	<Globals>
	</Globals>
</VisualStudioProject>
