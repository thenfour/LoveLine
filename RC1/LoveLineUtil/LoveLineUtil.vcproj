				Name="VCLinkerTool"
				AdditionalDependencies="wmstub.lib vfw32.lib wmvcore.lib odbc32.lib odbccp32.lib shlwapi.lib rpcrt4.lib dsound.lib quartz.lib strmbase.lib comctl32.lib"
